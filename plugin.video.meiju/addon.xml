<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.meiju"
    name="美剧"
    version="1.0.2"
    provider-name="yfang1644">
    <requires>
        <import addon="script.module.xbmcswift2" version="2.4.0"/>
        <import addon="script.module.beautifulsoup4" version="4.3.2"/>
        <import addon="script.module.geturl" version="1.0.0"/>
    </requires>
    <extension
        point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en">meiju video plugin</summary>
        <summary lang="zh">搜罗网络美剧</summary>
        <description lang="en">online video from yyetss(not yyets) and ttkmj, etc</description>
        <description lang="zh">网上美剧视频</description>
        <platform>all</platform>
        <forum></forum>
        <source>https://github.com/yfang1644/kodi_plugins/plugin.video.meiju</source>
        </extension>
</addon>
